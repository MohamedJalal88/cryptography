import numpy as np

P_matrix = np.array([[7, 19], [8, 7]])
real_key = np.array([[9, 4], [5, 7]])
C_matrix = np.dot(real_key, P_matrix) % 26

det_p = int(round(np.linalg.det(P_matrix)))
det_p_inv = pow(det_p, -1, 26)

adj_p = np.array([[P_matrix[1,1], -P_matrix[0,1]], [-P_matrix[1,0], P_matrix[0,0]]])
P_inv = (det_p_inv * adj_p) % 26

Recovered_Key = np.dot(C_matrix, P_inv) % 26

print("Recovered Key Matrix:")
print(Recovered_Key)
